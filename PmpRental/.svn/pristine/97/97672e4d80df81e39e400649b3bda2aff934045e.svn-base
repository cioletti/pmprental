<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>PmpRental</display-name>

	<context-param>
		<param-name>flex.class.path</param-name>
		<param-value>/WEB-INF/flex/hotfixes,/WEB-INF/flex/jars</param-value>
	</context-param>

	<!-- Http Flex Session attribute and binding listener support -->
	<listener>
		<listener-class>flex.messaging.HttpFlexSession</listener-class>
	</listener>
	<listener>
		<listener-class>com.pmprental.util.CronImportStandardJob</listener-class>
	</listener>
	<listener>
		<listener-class>com.pmprental.read.StartReadBufferPL</listener-class>
	</listener>
	<session-config><session-timeout>60</session-timeout></session-config>
	<!-- MessageBroker Servlet -->
	<servlet>
		<servlet-name>MessageBrokerServlet</servlet-name>
		<servlet-class>flex.messaging.MessageBrokerServlet</servlet-class>
		<init-param>
			<param-name>services.configuration.file</param-name>
			<param-value>/WEB-INF/flex/services-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>flex.write.path</param-name>
			<param-value>/WEB-INF/flex</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
		<servlet>
		<description></description>
		<display-name>LoginUsuario</display-name>
		<servlet-name>LoginUsuario</servlet-name>
		<servlet-class>com.pmprental.controller.LoginUsuario</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginUsuario</servlet-name>
		<url-pattern>/LoginUsuario</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>GetImgInspecao</display-name>
		<servlet-name>GetImgInspecao</servlet-name>
		<servlet-class>com.pmprental.controller.GetImgInspecao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetImgInspecao</servlet-name>
		<url-pattern>/GetImgInspecao</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ReportPdf</display-name>
		<servlet-name>ReportPdf</servlet-name>
		<servlet-class>com.pmprental.controller.ReportPdf</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		InspecaoReport</display-name>
		<servlet-name>InspecaoReport</servlet-name>
		<servlet-class>
		com.pmprental.controller.InspecaoReport</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		DownloadFileEt</display-name>
		<servlet-name>DownloadFileEt</servlet-name>
		<servlet-class>
		com.pmprental.controller.DownloadFileEt</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		ReportPmp</display-name>
		<servlet-name>ReportPmp</servlet-name>
		<servlet-class>
		com.pmprental.controller.ReportPmp</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServicoImportacao</display-name>
		<servlet-name>ServicoImportacao</servlet-name>
		<servlet-class>com.pmprental.controller.ServicoImportacao</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>MapaServlet</display-name>
		<servlet-name>MapaServlet</servlet-name> 
		<servlet-class>com.pmprental.controller.MapaServlet</servlet-class>
	</servlet>
	
	<servlet>
		<description>
		</description>
		<display-name>CancelarEnvioEmail</display-name>
		<servlet-name>CancelarEnvioEmail</servlet-name> 
		<servlet-class>com.pmprental.controller.CancelarEnvioEmail</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ReportCusto</display-name>
		<servlet-name>ReportCusto</servlet-name>
		<servlet-class>com.pmprental.controller.ReportCusto</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ReportMaquinasSemContrato</display-name>
		<servlet-name>ReportMaquinasSemContrato</servlet-name>
		<servlet-class>com.pmprental.controller.ReportMaquinasSemContrato</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MessageBrokerServlet</servlet-name>
		<url-pattern>/messagebroker/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportPdf</servlet-name>
		<url-pattern>/ReportPdf</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InspecaoReport</servlet-name>
		<url-pattern>/InspecaoReport</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DownloadFileEt</servlet-name>
		<url-pattern>/DownloadFileEt</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportPmp</servlet-name>
		<url-pattern>/ReportPmp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServicoImportacao</servlet-name>
		<url-pattern>/ServicoImportacao</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MapaServlet</servlet-name>
		<url-pattern>/MapaServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CancelarEnvioEmail</servlet-name>
		<url-pattern>/CancelarEnvioEmail</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportCusto</servlet-name>
		<url-pattern>/ReportCusto</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportMaquinasSemContrato</servlet-name>
		<url-pattern>/ReportMaquinasSemContrato</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>PmpRental.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

