	 <description>JDBC Stratec Datasource</description>
	 <res-ref-name>jdbc/stratec</res-ref-name>
	 <res-type>javax.sql.DataSource</res-type>
	 <res-auth>Container</res-auth>
	</resource-ref>
</web-app>
